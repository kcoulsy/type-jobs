{
  "name": "typed-jobs-example",
  "version": "1.0.0",
  "description": "Simple Express example using typed-jobs",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"tsx watch server.ts\" \"tsx ../../dist/cli.js run\"",
    "start": "node dist/server.js",
    "build": "tsc",
    "worker": "tsx ../../dist/cli.js run"
  },
  "dependencies": {
    "bullmq": "^5.65.1",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "ioredis": "^5.8.2",
    "typed-jobs": "file:../../"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "concurrently": "^9.2.1",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  }
}
